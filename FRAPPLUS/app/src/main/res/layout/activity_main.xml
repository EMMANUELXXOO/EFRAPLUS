<?xml version="1.0" encoding="utf-8"?>
<androidx.coordinatorlayout.widget.CoordinatorLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">
    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        >
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="84dp"
            tools:ignore="ExtraText">

            <Button
                android:id="@+id/BtnInsertar"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="16dp"

                android:onClick="onRequestButtonClicked"
                android:text="Realizar Solicitud"
                android:translationX="0dp" />

            <EditText
                android:id="@+id/txtid"
                android:layout_width="245dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="16dp"

                android:hint="Id"
                android:inputType="text" />



        </LinearLayout>
    </LinearLayout>

    <LinearLayout
        android:layout_width="335dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="60dp">

        <EditText
            android:id="@+id/txtestado"
            android:layout_width="245dp"
            android:layout_height="wrap_content"
            android:layout_marginLeft="90dp"
            android:layout_marginTop="16dp"
            android:hint="Estado"
            android:inputType="text" />
    </LinearLayout>



<LinearLayout
    android:layout_width="wrap_content"
    android:layout_height="wrap_content">



    <TextView
        android:id="@+id/textviewregister"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Secciones De FRAP"
        android:textSize="18sp"
        android:layout_marginTop="130dp"
        android:layout_marginLeft="120dp"
        android:textStyle="bold"

        />
</LinearLayout>


    <LinearLayout
        android:id="@+id/linearLayout2"
        android:layout_width="match_parent"
        android:layout_marginTop="100dp"
        android:layout_height="match_parent">


        <Button

            android:layout_width="180dp"
            android:layout_height="55dp"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="75dp"
            android:layout_marginRight="10dp"
            android:text="I.Delegacion"

            android:textColor="@color/white"
            android:textStyle="bold" />

        <Button

            android:layout_width="180dp"
            android:layout_height="55dp"
            android:layout_marginLeft="0dp"
            android:layout_marginTop="75dp"
            android:layout_marginRight="10dp"
            android:text="IX.Evaluacion II"
            android:textColor="@color/white"
            android:textStyle="bold" />

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:layout_anchor="@+id/linearLayout2"
        app:layout_anchorGravity="center"
        android:layout_marginTop="100dp"
        >


        <Button

            android:layout_width="180dp"
            android:layout_height="55dp"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="160dp"
            android:layout_marginRight="10dp"
            android:text="II.Servicio"
            android:textColor="@color/white"
            android:textStyle="bold" />

        <Button

            android:layout_width="180dp"
            android:layout_height="55dp"
            android:layout_marginLeft="0dp"
            android:layout_marginTop="160dp"
            android:layout_marginRight="10dp"
            android:text="X.Tratamiento"
            android:textColor="@color/white"
            android:textStyle="bold" />



    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:layout_anchor="@+id/linearLayout2"
        app:layout_anchorGravity="center"
        android:layout_marginTop="100dp"
        >


        <Button

            android:layout_width="180dp"
            android:layout_height="55dp"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="250dp"
            android:layout_marginRight="10dp"
            android:text="III.Control"
            android:textColor="@color/white"
            android:textStyle="bold" />

        <Button

            android:layout_width="180dp"
            android:layout_height="55dp"
            android:layout_marginLeft="0dp"
            android:layout_marginTop="250dp"
            android:layout_marginRight="10dp"
            android:text="XI.Traslado"
            android:textColor="@color/white"
            android:textStyle="bold" />

    </LinearLayout>


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:layout_anchor="@+id/linearLayout2"
        app:layout_anchorGravity="center"
        android:layout_marginTop="100dp">


        <Button

            android:layout_width="180dp"
            android:layout_height="55dp"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="340dp"
            android:layout_marginRight="10dp"
            android:text="IV.Paciente"
            android:textColor="@color/white"
            android:textStyle="bold" />

        <Button

            android:layout_width="180dp"
            android:layout_height="55dp"
            android:layout_marginLeft="0dp"
            android:layout_marginTop="340dp"
            android:layout_marginRight="10dp"
            android:text="XII.Observaciones"
            android:textColor="@color/white"
            android:textStyle="bold" />

    </LinearLayout>


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:layout_anchor="@+id/linearLayout2"
        app:layout_anchorGravity="center"
        android:layout_marginTop="100dp"
        >


        <Button

            android:layout_width="180dp"
            android:layout_height="55dp"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="430dp"
            android:layout_marginRight="10dp"
            android:text="V.Causa Tx"
            android:textColor="@color/white"
            android:textStyle="bold" />

        <Button

            android:layout_width="180dp"
            android:layout_height="55dp"
            android:layout_marginLeft="0dp"
            android:layout_marginTop="420dp"
            android:layout_marginRight="10dp"
            android:text="XIII.Ministerio Publico"
            android:textColor="@color/white"
            android:textStyle="bold" />

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:layout_anchor="@+id/linearLayout2"
        app:layout_anchorGravity="center"
        android:layout_marginTop="100dp">


        <Button

            android:layout_width="180dp"
            android:layout_height="55dp"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="520dp"
            android:layout_marginRight="10dp"
            android:text="VI.Causa Clinica"
            android:textColor="@color/white"
            android:textStyle="bold" />

        <Button

            android:layout_width="180dp"
            android:layout_height="55dp"
            android:layout_marginLeft="0dp"
            android:layout_marginTop="520dp"
            android:layout_marginRight="10dp"
            android:text="XIV.Datos Legales"
            android:textColor="@color/white"
            android:textStyle="bold" />

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:layout_anchor="@+id/linearLayout2"
        app:layout_anchorGravity="center"
        android:layout_marginTop="100dp"
        >


        <Button

            android:layout_width="180dp"
            android:layout_height="55dp"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="610dp"
            android:layout_marginRight="10dp"
            android:text="VII.Parto"
            android:textColor="@color/white"
            android:textStyle="bold" />

        <Button

            android:layout_width="180dp"
            android:layout_height="55dp"
            android:layout_marginLeft="0dp"
            android:layout_marginTop="610dp"
            android:layout_marginRight="10dp"
            android:text="XV.Hospital Receptor"
            android:textColor="@color/white"
            android:textStyle="bold" />

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="935dp"
        android:layout_marginTop="100dp"
        app:layout_anchor="@+id/linearLayout2"
        app:layout_anchorGravity="center">


        <Button

            android:layout_width="180dp"
            android:layout_height="55dp"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="700dp"
            android:layout_marginRight="10dp"
            android:text="VIII.Evaluacion I"
            android:textColor="@color/white"
            android:textStyle="bold" />

        <Button

            android:layout_width="180dp"
            android:layout_height="55dp"
            android:layout_marginLeft="0dp"
            android:layout_marginTop="700dp"
            android:layout_marginRight="10dp"
            android:text="Consumos"
            android:textColor="@color/white"
            android:textStyle="bold" />

    </LinearLayout>

</androidx.coordinatorlayout.widget.CoordinatorLayout>